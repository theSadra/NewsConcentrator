@model NewsConcentratorSystem.Models.TelegramChannel

@{
    ViewData["Title"] = "Edit";
}
<h1>@Model.ChannelUserName</h1>
<div>
    <a asp-action="Index">بازگشت به لیست</a>
</div>
<hr />
<div class="row" style="direction: ltr;">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ChannelId" />
            <div class="form-group">
                <label asp-for="IntervalMins" class="control-label"></label>
                <input asp-for="IntervalMins" class="form-control" />
                <span asp-validation-for="IntervalMins" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="ذخیره تایم" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div class="row mustcon">
    
    
    
    
    

</div>
<hr />




@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
